{
  "name": "a-front-end-service",
  "version": "1.0.0",
  "description": "An example frontend service for Felles Datakatalog",
  "private": true,
  "engines": {
    "node": ">= 12.13.0"
  },
  "scripts": {
    "start": "webpack-dev-server  --hot --inline --config-register @babel/register --config webpack/dev.config.js",
    "start:with-mock": "npm-run-all -p mock:start:d start",
    "mock:start": "cd mock && bash start-mock.sh",
    "mock:start:d": "cd mock && bash start-mock-d.sh",
    "mock:stop": "cd mock && docker-compose down",
    "lint": "eslint",
    "lint:project": "eslint --ext tsx,ts,jsx,js,json,html ./",
    "lint:staged:fix": "eslint --ext tsx,ts,jsx,js,json,md,html --fix",
    "commit": "git-cz",
    "build:prod": "npm-run-all build:webpack:prod",
    "build:webpack:prod": "webpack --config-register @babel/register --config webpack/prod.config.js",
    "test": "jest",
    "test:i": "jest --watch",
    "test:feature": "codeceptjs run --features",
    "test:feature:tag": "codeceptjs run --grep",
    "test:feature:debug": "codeceptjs run --debug",
    "test:feature:steps": "codeceptjs run --steps",
    "test:feature:init": "codeceptjs gherkin:init",
    "test:gen:feature": "bash test/utils/newFeature.sh",
    "test:gen:steps": "codeceptjs gherkin:snippets",
    "build": "webpack --config-register @babel/register --config webpack/prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Informasjonsforvaltning/a-front-end-service.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/register": "^7.7.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@openapitools/openapi-generator-cli": "^1.0.8-4.2.2",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-jsx-remove-data-test-id": "^2.1.3",
    "babel-plugin-styled-components": "^1.10.6",
    "base-href-webpack-plugin": "^2.0.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "codeceptjs": "^2.3.6",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gherkin": "^8.2.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "install": "^0.13.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "npm": "^6.13.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "puppeteer": "^2.0.0",
    "react-hot-loader": "^4.12.18",
    "react-test-renderer": "^16.12.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.7.1",
    "core-js": "^3.4.5",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "regenerator-runtime": "^0.13.3",
    "ts-jest": "^24.2.0"
  }
}
